<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar" controllerName="br.com.cliente.gestao.fiorireport278.controller.DataBinding" displayBlock="true">
	<Page id="DataBinding" title="{i18n>title}">
		<content>
			<Panel xmlns="sap.m" width="auto" expandable="true" expanded="true" id="panel0">
				<headerToolbar>
					<Toolbar id="toolbar1">
						<Title text="Model via serviço OData" id="headerTitle"/>
						<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
						<Button id="_IDGenButton1" icon="sap-icon://settings"/>
					</Toolbar>
				</headerToolbar>

				<content>
					<List noDataText="Sem produtos disponíveis" id="list0" items="{/Products}">
					<items>
					    <StandardListItem type="Navigation" title="{ProductId}" description="{Name}" icon="sap-icon://picture" id="item0"/>
					</items>
					</List>
				</content>

				<infoToolbar>
					<Toolbar width="100%" id="toolbarInfo">
						<content>
							<Text xmlns="sap.m" text="Informação Adicional List" id="text0"/>
						</content>
					</Toolbar>
				</infoToolbar>
			</Panel>

		</content>
	</Page>
</mvc:View>